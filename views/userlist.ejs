<!doctype html>
<html>
  <head>
    <title><%=healthAppData.appName%> Users</title>
    <link rel="stylesheet"  type="text/css" href="/main.css" />
  </head>
  <body>
    <h1><%=healthAppData.appName%> User List</h1>
    <h2>Current users:</h2>
    <table>
    <tr>
      <th>Username</th>
      <th>First name</th>
      <th>Last name</th>
    </tr>
    <% users.forEach(function(user) { %>
        <tr>
          <td><%= user.username %></td>
          <td><%= user.firstname %></td>
          <td><%= user.lastname %></td>
        </tr>
    <% }); %>
    </table>
  </body>
</html>